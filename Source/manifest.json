{
	"manifest_version": 2,
	"name": "Jaquaysing the Internet",
	"version": "1.0",
        "icons": {
                "16": "logo16.png",
                "32": "logo32.png",
                "48": "logo48.png",
                "128": "logo128.png"
        },
	"description": "Replaces the text 'Xandering' with 'Jaquaysing'.",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["jaquaysing.js"],
			"run_at": "document_end"
		}
	]
}
